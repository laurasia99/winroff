WINFONTS = $(WIN10FONTS) $(WIN10FOD) $(MSOFFICE) $(MSOTHER)

all:: $U\DESC $(URWFONTS) $(CMFONTS) $(CMUFONTS) $(WINFONTS)
    @echo Computer Modern fonts:
    @echo $(CMFONTS) $(CMUFONTS)
    @echo Core system fonts:
    @echo $(WIN10FONTS)
    @echo Additional fonts:
    @echo $(WIN10FOD)
    @echo Microsoft office fonts:
    @echo $(MSOFFICE)
    @echo Other Microsoft fonts:
    @echo $(MSOTHER)

clean::
    if exist devutf rmdir /s /q devutf

$U\DESC:
    @if not exist $U mkdir $U
    echo fonts 10 R I B BI CR HR HI HB S1 S >$@
    echo res $(RES) >>$@
    echo hor 1 >>$@
    echo ver 1 >>$@
    echo unitwidth 10 >>$@

# Standard fonts (URW35)

!IF 0
# Eventually use the TrueType fonts -- currently having problems
#    @if not exist $U mkdir $U
#    .\mkfn.exe -b -l -o -r$(RES) $(SCR) -t$(@B) -f $W\$(@B).ttf > $@
$U\AB: $F\URWGothic-Demi.ttf
    @if not exist $U mkdir $U
    .\mkfn.exe -b -l -o -r$(RES) $(SCR) -t$(@B) -f $F\URWGothic-Demi.ttf -pAvantGarde-Demi > $@
$U\AI: $F\URWGothic-BookOblique.ttf
    @if not exist $U mkdir $U
    .\mkfn.exe -b -l -o -r$(RES) $(SCR) -t$(@B) -f $F\URWGothic-BookOblique.ttf -pAvantGarde-BookOblique > $@
$U\AR: $F\URWGothic-Book.ttf
    @if not exist $U mkdir $U
    .\mkfn.exe -b -l -o -r$(RES) $(SCR) -t$(@B) -f $F\URWGothic-Book.ttf -pAvantGarde-Book > $@
$U\AX: $F\URWGothic-DemiOblique.ttf
