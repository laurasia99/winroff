!IFNDEF _NMAKE_DEVUTF_INCLUDED
_NMAKE_DEVUTF_INCLUDED=1

# mkfn.exe
!IF EXIST(mkfn\NMakefile)
!INCLUDE .\mkfn\NMakefile
!ELSE
!ERROR 'mkfn' required to build 'devutf'
!ENDIF

# Default fonts
F=.\fonts

# device resolution
RES=720
# scripts to include (latn,arab) [OpenType]
SCR=-Slatn
# pattern of ligatures to ignore [relies on sed filter]
#LIGIGN=(ct|st|sp|Rp)

# Windows System and User font directory
W = $(WINDIR)\fonts
V = $(LOCALAPPDATA)\Microsoft\Windows\Fonts
!MESSAGE Windows System font directory $W
!MESSAGE Windows User font directory $V

U=devutf

# Note inclusion of shortcuts
URWFONTS = $U\AB $U\AI $U\AR $U\AX $U\B $U\BI $U\CB $U\CI $U\CR $U\CX \
        $U\HB $U\HI $U\HR $U\HX $U\I $U\KB $U\KI $U\KR $U\KX $U\NB \
        $U\NI $U\NR $U\NX $U\PA $U\PB $U\PI $U\PR $U\PX $U\R $U\S \
        $U\S1 $U\ZI $U\HNB $U\HNI $U\HNR $U\HNX \
        $U\C059-BdIta $U\C059-Bold $U\C059-Italic $U\C059-Roman \
        $U\D050000L \
        $U\NimbusMonoPS-Bold $U\NimbusMonoPS-BoldItalic \
        $U\NimbusMonoPS-Italic $U\NimbusMonoPS-Regular \
        $U\NimbusRoman-Bold $U\NimbusRoman-BoldItalic \
        $U\NimbusRoman-Italic $U\NimbusRoman-Regular \
